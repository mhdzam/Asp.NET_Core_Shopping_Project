@model IEnumerable<Shop.Application.Cart.GetCart.Response>
@{ 

}



    <div>
        <h1>Big Cart Component</h1>
        @foreach (var product in Model)
        {
            <p>@product.Name</p>
            <p>@product.Value</p>
            <p>@product.Qty</p>
            <p>@product.StockId</p>
            <hr />
        }

    </div>